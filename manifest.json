{
  "name": "Bing Rewards Search Extension",
  "version": "1.2",
  "manifest_version": 2,
  "options_page": "options.html",
  "description": "Bing Rewards auto search script.",
  "browser_action": {
	"default_icon": "icon.png"
  },
  "background": {
    "scripts": ["search.js"]
  },
  "permissions": [
    "tabs"
  ]
}